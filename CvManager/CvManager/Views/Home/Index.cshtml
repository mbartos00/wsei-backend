@using Microsoft.AspNetCore.Identity
@inject SignInManager<IdentityUser> SignInManager


@{
    ViewData["Title"] = "Home Page";
}




@if (SignInManager.IsSignedIn(User))
{

    <a style="background-color: #dbdbdb; padding: 1.2rem 3rem; border-radius: 10px; color: black; text-decoration: none" asp-area="" asp-controller="Cv" asp-action="Index">Cv manager</a>

}